
row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")

split_positions = list(position)
column = split_positions[1]
row = split_positions[0]
map[int(column) - 1][int(row) - 1] = "x"

print(f"{row1}\n{row2}\n{row3}")
